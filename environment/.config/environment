getenv EDITOR nvim
getenv VISUAL nvim
getenv TERM xterm-256color
getenv MANPAGER less
getenv MANPATH /usr/share/man
getenv LESSHISTFILE /dev/null
getenv MOZ_ENABLE_WAYLAND firefox
getenv FZF_DEFAULT_COMMAND 'fd --type f --hidden --follow --exclude .git'
getenv FZF_DEFAULT_OPTS '--height 40% --reverse'
getenv _ZL_DATA ~/.local/share/.zlua
getenv _ZL_ADD_ONCE 1
getenv GTK_IM_MODULE fcitx
getenv QT_IM_MODULE fcitx
getenv XMODIFIERS  @im=fcitx

getalias la 'ls -Alh --group-directories-first --file-type --color=auto'
getalias kill "ps -ef | fzf | awk '{print $2}' | xargs kill -9"

getpath ~/Documents/Exec
